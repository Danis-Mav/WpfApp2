<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1200">
    

    
    <Border Background="Aquamarine" BorderBrush="Gainsboro" BorderThickness="1">
        <Grid>

            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition Width="10*"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition Height="10*"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Image Grid.ColumnSpan="3" Grid.RowSpan="3" Source="Image2.png" Stretch="Fill"/>
            <StackPanel Grid.Column="1" Grid.Row="1" Background="CornflowerBlue">
                <Frame x:Name="frame_autho_reg" NavigationUIVisibility="Hidden"/>
                <Button x:Name="backBtn" Width="30" Height="30" HorizontalAlignment="Left" Background="White" Click="backBtn_Click" Margin="5,605,0,600"/>
            </StackPanel>
            
        </Grid>
    </Border>
</Window>
